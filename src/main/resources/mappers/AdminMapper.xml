<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.deft.crud.admin.model.dao.AdminMapper">

	<resultMap type="com.deft.crud.member.model.dto.MemberDTO" id="memberesultMap">
		<id property="empNo" column="EMP_NO"/>
		<result property="empName" column="EMP_NAME"/>
		<result property="empId" column="EMP_ID"/>
		<result property="empPwd" column="EMP_PWD"/>
		<result property="empEmail" column="EMP_EMAIL"/>
		<result property="gender" column="GENDER"/>
		<result property="empBirth" column="EMP_BIRTH"/>
		<result property="hireYn" column="HIRE_YN"/>
		<result property="authority" column="AUTHORITY"/>
		<result property="hireDate" column="HIRE_DATE"/>
		<result property="entDate" column="ENT_DATE"/>
		<result property="empAddress" column="EMP_ADDRESS"/>
		<result property="empPhone" column="EMP_PHONE"/>
		<result property="empTel" column="EMP_TEL"/>
		<result property="empFax" column="EMP_FAX"/>
		<result property="jobNo" column="JOB_NO"/>
		<result property="deptNo" column="DEPT_NO"/>
		<result property="managerNo" column="MANAGER_NO"/>
	</resultMap>

	<select id="checkUserId" parameterType="String" resultType="_int">
		SELECT
		       COUNT(*)
		  FROM EMP_INFO
		 WHERE EMP_ID = #{userId}     
	</select>

	<insert id="insertMember" parameterType="com.deft.crud.member.model.dto.MemberDTO">
		INSERT
		  INTO EMP_INFO A
		 (
		   A.EMP_NO
		 , A.EMP_NAME  
		 , A.EMP_ID  
		 , A.EMP_PWD  
		 , A.EMP_EMAIL  
		 , A.GENDER  
		 , A.EMP_BIRTH  
		 , A.AUTHORITY  
		 , A.HIRE_DATE  
		 , A.ENT_DATE  
		 , A.EMP_PHONE  
		 , A.JOB_NO  
		 , A.DEPT_NO  
		 , A.MANAGER_NO  
		 ) 
		  VALUES
		 (
		   #{empNo}
		 , #{empName}
		 , #{empId}
		 , #{empPwd}
		 , #{empEmail}
		 , #{gender}
		 , #{empBirth}  
		 , #{authority}  
		 , #{hireDate}  
		 , #{entDate}  
		 , #{empPhone}  
		 , #{jobNo}  
		 , #{deptNo}  
		 , #{managerNo}  
		 )
	</insert>
</mapper>

















